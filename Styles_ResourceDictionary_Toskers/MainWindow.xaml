<Window x:Class="Styles_ResourceDictionary_Toskers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Styles_ResourceDictionary_Toskers"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="50">
            <Label Content="BIG TITLE" Style="{StaticResource TitleLabel}" />
            <Label Content="Full Name" Style="{StaticResource TextboxLabel}" />
            <TextBox Style="{StaticResource TxtBox}"/>
            <Button Style="{StaticResource SubmitButton}" >Submit</Button>
            <Button Style="{StaticResource CancelButton}">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
